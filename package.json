{
  "name": "graasp-account",
  "version": "2.6.2",
  "private": true,
  "contributors": [
    "Kim Lan Phan Hoang",
    "Mariem Bencheikh"
  ],
  "engines": {
    "node": ">=20"
  },
  "type": "module",
  "dependencies": {
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "@graasp/query-client": "3.6.0",
    "@graasp/sdk": "4.9.0",
    "@graasp/translations": "1.28.0",
    "@graasp/ui": "4.17.1",
    "@mui/icons-material": "5.15.16",
    "@mui/lab": "5.0.0-alpha.170",
    "@mui/material": "5.15.16",
    "@uppy/core": "^3.11.1",
    "@uppy/dashboard": "3.8.2",
    "@uppy/drag-drop": "3.1.0",
    "@uppy/file-input": "3.1.2",
    "@uppy/progress-bar": "3.1.1",
    "@uppy/react": "3.3.1",
    "@uppy/status-bar": "^3.3.2",
    "@uppy/tus": "3.5.4",
    "@uppy/xhr-upload": "3.6.6",
    "axios": "1.6.8",
    "filesize": "^10.1.1",
    "http-status-codes": "2.3.0",
    "i18next": "23.11.3",
    "lodash.truncate": "4.4.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "^14.1.1",
    "react-image-crop": "11.0.5",
    "react-router": "6.23.0",
    "react-router-dom": "6.23.0",
    "react-toastify": "10.0.5",
    "social-links": "^1.14.0",
    "stylis-plugin-rtl": "2.1.1",
    "uuid": "9.0.1"
  },
  "scripts": {
    "dev": "vite",
    "start": "yarn dev",
    "start:test": "vite --mode test",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:test": "vite build --mode test",
    "preview": "vite preview",
    "preview:dev": "vite preview --mode development",
    "preview:test": "vite preview --mode test",
    "lint": "eslint .",
    "pre-commit": "yarn prettier:check && yarn lint && yarn type-check",
    "prettier:check": "prettier --check src/**/*.{js,ts,tsx}",
    "prettier:write": "prettier --write src/**/*.{js,ts,tsx}",
    "type-check": "tsc --noEmit",
    "check": "yarn prettier:check && yarn lint && yarn type-check",
    "hooks:install": "husky",
    "cypress:open": "env-cmd -f ./.env.test cypress open --browser chrome",
    "cypress": "yarn test",
    "test": "yarn build:test && concurrently -k -s first \"yarn preview:test\" \"yarn cypress:run\"",
    "cypress:run": "env-cmd -f ./.env.test cypress run --browser chrome"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@cypress/code-coverage": "3.12.38",
    "@cypress/instrument-cra": "1.4.0",
    "@testing-library/jest-dom": "6.4.5",
    "@testing-library/react": "15.0.6",
    "@testing-library/user-event": "14.5.2",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/react-toastify": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "@vitejs/plugin-react": "4.2.1",
    "concurrently": "8.2.2",
    "cypress": "13.9.0",
    "env-cmd": "10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "husky": "9.0.11",
    "prettier": "3.2.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "5.4.5",
    "vite": "5.2.11",
    "vite-plugin-checker": "0.6.4",
    "vite-plugin-istanbul": "6.0.0"
  },
  "packageManager": "yarn@4.2.1"
}
